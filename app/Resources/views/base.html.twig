<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Auta{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link rel="stylesheet"  href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.min.css') }}">
        <link rel="stylesheet"  href="{{ asset('bundles/app/css/main.css') }}">
        <link rel="stylesheet"  href="{{ asset('assets/vendor/angular-toastr/dist/angular-toastr.css') }}">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">

        <script src="{{ asset('assets/vendor/jquery/dist/jquery.js') }}"></script>
        <script src="{{ asset('assets/vendor/angular/angular.js') }}"></script>
        <script src="{{ asset('assets/vendor/angular-route/angular-route.min.js') }}"></script>
        <script src="{{ asset('assets/vendor/angular-resource/angular-resource.min.js') }}"></script>
        <script src="{{ asset('assets/vendor/angular-toastr/dist/angular-toastr.js') }}"></script>

        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
    </head>
    <body data-ng-app="app">
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}

        <script src="{{ asset('bundles/app/js/modules/notes/controller.js') }}"></script>
    </body>

</html>
